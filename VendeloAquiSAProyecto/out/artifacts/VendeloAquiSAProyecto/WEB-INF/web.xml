<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



    <servlet>
        <servlet-name>ManejadorProveedores</servlet-name>
        <servlet-class>VendeloAquiPack.ManejadorProveedores</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ManejadorProveedores</servlet-name>
        <url-pattern>/ManejadorProveedores</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ManejadorProductos</servlet-name>
        <servlet-class>VendeloAquiPack.ManejadorProductos</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ManejadorProductos</servlet-name>
        <url-pattern>/ManejadorProductos</url-pattern>
    </servlet-mapping>




    <servlet>
        <servlet-name>indexsrvlt</servlet-name>
        <servlet-class>VendeloAquiPack.indexsrvlt</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>indexsrvlt</servlet-name>
        <url-pattern>/indexsrvlt</url-pattern>
    </servlet-mapping>



</web-app>